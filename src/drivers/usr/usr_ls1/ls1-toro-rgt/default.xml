<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ls1-toro-rgt" type="template" mode="mw">
      <!-- Car parameters >>> -->
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="21" val="21.0"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="24" val="17.4"/>
	</section>

      <!-- Shift at 7003 rpm -->
	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2.0"/>
			</section>

			<section name="1">
				<attnum name="ratio" min="0" max="5" val="3.01"/>
			</section>

			<section name="2">
				<attnum name="ratio" min="0" max="5" val="2.29"/>
			</section>

			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.76"/>
			</section>

			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.3"/>
			</section>

			<section name="5">
				<attnum name="ratio" min="0" max="5" val="1.04"/>
			</section>

			<section name="6">
				<attnum name="ratio" min="0" max="5" val="0.8"/>
			</section>
		</section>
	</section>

      <!-- For top speed without wings ~338 km/h >>> -->
	<section name="Rear Differential">
		<attstr name="type" in="LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="1.00" max="10.00" val="3.5"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.31"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="34000"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="200" max="2500" val="1330"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="1" max="2500" val="1"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="130" val="90"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="-0.0"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="130" val="90"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0.0"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="130" val="90"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="0.0"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="130" val="90"/>
		<attnum name="toe" unit="deg" min="-1" max="1" val="-0.0"/>
		<attnum name="camber" min="-5" max="5" unit="deg" val="-5"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" min="100" max="10000" unit="lbs/in" val="3500"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.20"/>
		<attnum name="bellcrank" min="0.1" max="2.0" val="0.8"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="95"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="85"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="275"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" min="100" max="10000" unit="lbs/in" val="3500"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.20"/>
		<attnum name="bellcrank" min="0.1" max="2.0" val="0.8"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="95"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="400"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="85"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="275"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" min="100" max="10000" unit="lbs/in" val="5000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.20"/>
		<attnum name="bellcrank" min="0.1" max="2.0" val="0.82"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="130"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="280"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="110"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="170"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" min="100" max="10000" unit="lbs/in" val="5000"/>
		<attnum name="suspension course" unit="m" min="0" max="0.28" val="0.20"/>
		<attnum name="bellcrank" min="0.1" max="2.0" val="0.82"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="130"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="280"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="400" val="110"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="170"/>
	</section>
      <!-- <<< Car parameters -->

	<section name="private">
		<attnum name="min corner inverse" min="0" max="2" val="0.001"/>
		<attnum name="increase corner inverse" min="0" max="2" val="0.040"/>
		<attnum name="add avoid speed" min="-20" max="20" val="0.0"/>
		<attnum name="avoid speed factor" min="-20" max="20" val="1.25"/>
		<attnum name="avoid brake factor" min="-20" max="20" val="1.2"/>
		<attnum name="exit boost factor" min="-20" max="20" val="1.15"/>
		<attnum name="avoid exit boost factor" min="-20" max="20" val="1.2"/>
		<attnum name="brake margin" min="-20" max="20" val="-20"/>
		<attnum name="base speed factor" min="0.0" max="50.0" val="1.50"/>
		<attnum name="base brake factor" min="0.0" max="50.0" val="1.05"/>
		<attnum name="base speed" min="0.0" max="50.0" val="14.00"/>
		<attnum name="base brake" min="10.0" max="100.0" val="25.00"/>
		<attnum name="brake mod" min="0.0" max="1.0" val="1.00"/>
		<attnum name="steer mod" min="0.0" max="1.0" val="1.00"/>
		<attnum name="slope factor" min="0.0" max="1.0" val="0.50"/>
		<attnum name="accel exit" min="0.0" max="10.0" val="0.65"/>
		<attnum name="turn decel" min="0" max="2" val="0.45"/>
		<attnum name="curve factor" min="0" max="2" val="0.11"/>
		<attnum name="accel curve" min="0" max="2" val="1.20"/>
		<attnum name="accel curve limit" min="0" max="6" val="1.5"/>
		<attnum name="brake curve limit" min="0" max="6" val="3"/>
		<attnum name="revs change up" min="0" max="2" val="0.8"/>
		<attnum name="revs change down" min="0" max="2" val="0.72"/>
		<attnum name="revs change down max" min="0" max="2" val="0.75"/>
		<attnum name="steer skid" min="0" max="3" val="0.0"/>
		<attnum name="steer gain" min="0" max="3" val="1.5"/>
		<attnum name="max steer time" min="0.5" max="10" val="2.3"/>
		<attnum name="min steer time" min="0.5" max="10" val="1.75"/>
		<attnum name="int margin" min="0" max="2" val="1.7"/>
		<attnum name="ext margin" min="0" max="2" val="1.3"/>
		<attnum name="no team waiting" min="0" max="2" val="1"/>
		<attnum name="inc factor" min="0" max="5" val="0.1"/>
		<attnum name="overtake caution" min="-20" max="20" val="0.2"/>
		<attnum name="abs slip" min="0.5" max="4.5" val="4.0"/>
		<attnum name="abs range" min="0.5" max="10.5" val="9.0"/>
		<attnum name="tcl slip" min="0.5" max="8.5" val="6.5"/>
		<attnum name="skid correction" min="0.0" max="8.5" val="0.5"/>
		<attnum name="bump caution" min="0.0" max="4.0" val="0.7"/>
		<attnum name="oversteer asr" min="-0.0" max="2.0" val="0.2"/>
		<attnum name="side margin" min="0.0" max="4.0" val="1.0"/>
		<attnum name="pit offset" min="-200.0" max="200.0" val="-50"/>

		<attnum name="steer debug" min="-5" max="5" val="-1.0"/>
		<attnum name="overtake debug" min="0" max="1" val="0"/>
		<attnum name="raceline debug" min="0" max="1" val="0"/>
	</section>
</params>
