<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="Archer R9" type="template">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="7" max="11" val="11"/>
	</section>

	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="8" max="24" val="20"/>
	</section>

	<section name="Gearbox">
		<section name="gears">
		<section name="r">
			<attnum name="ratio" min="-3" max="0" val="-3.0"/>
		</section>

		<section name="1">
			<attnum name="ratio" min="0" max="5" val="3.0"/>
		</section>

		<section name="2">
			<attnum name="ratio" min="0" max="5" val="2.05"/>
		</section>

		<section name="3">
			<attnum name="ratio" min="0" max="5" val="1.41"/>
		</section>

		<section name="4">
			<attnum name="ratio" min="0" max="5" val="1.07"/>
		</section>

		<section name="5">
			<attnum name="ratio" min="0" max="5" val="0.96"/>
		</section>

		<section name="6">
			<attnum name="ratio" min="0" max="5" val="0.81"/>
		</section>
	</section>
	</section>
	
	<section name="Steer">
		<attnum name="steer lock" unit="deg" min="1" max="23" val="23"/>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.6"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="45000"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="90"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.3"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3.2"/>
		<attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="90"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.3"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-3.2"/>
		<attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="100"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-2.5"/>
		<attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="60" max="300" val="100"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-2.5"/>
		<attnum name="pressure" unit="PSI" min="14.0" max="45.0" val="25.0"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="200" max="2300" val="650"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="1" max="700" val="380"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="800" max="2200" val="1000"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="280"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="320"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="200" val="40"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" min="800" max="2200" val="1000"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="280"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="320"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="200" val="40"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="400" max="2200" val="650"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="115"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="80"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="200" val="15"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="400" max="2200" val="650"/>
		<attnum name="packers" unit="mm" min="0" max="10" val="0"/>
		<attnum name="slow bump" unit="lbs/in/s" min="0" max="400" val="115"/>
		<attnum name="slow rebound" unit="lbs/in/s" min="0" max="400" val="80"/>
		<attnum name="fast bump" unit="lbs/in/s" min="0" max="200" val="15"/>
		<attnum name="fast rebound" unit="lbs/in/s" min="0" max="400" val="40"/>
	</section>

</params>














