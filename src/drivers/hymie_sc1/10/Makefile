ROBOT = hymie_sc1

SHIPDIR   = drivers/${ROBOT}/10

SHIP      =  *.rgb $(shell find *.xml -maxdepth 0 -type f -print)

src-robots-hymie_sc1_PKGFILES	= $(shell find * -maxdepth 0 -type f -print)

src-robots-hymie_sc1_PKGDIR	= ${PACKAGE}-${VERSION}/$(subst ${TORCSNG_BASE},,$(shell pwd))

include ${MAKE_DEFAULT}


