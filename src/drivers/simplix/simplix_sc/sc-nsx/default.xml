<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="sc-nsx" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" val="0"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" val="0"/>
	</section>
	<section name="Gearbox">
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-3" max="0" val="-2.0"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0" max="5" val="2.66"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0" max="5" val="1.78"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0" max="5" val="1.3"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0" max="5" val="1.02"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0" max="5" val=".84"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0" max="5" val="0.70"/>
			</section>
		</section>
	</section>
	<section name="Rear Differential">
		<!-- type of differential : SPOOL (locked), FREE, LIMITED SLIP -->
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"/>
		<attnum name="ratio" min="0" max="10" val="4.5"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.56"/>
		<attnum name="max pressure" min="100" max="150000" unit="kPa" val="40000"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" min="0" max="5000" unit="lbs/in" val="2000"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="2.5"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" min="0" max="5000" unit="lbs/in" val="1000"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="1" max="5" val="2.5"/>
	</section>
	<section name="Front Right Wheel">
		<attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
		<attnum name="camber" min="-2" max="0" unit="deg" val="-2"/>
	</section>
	<section name="Front Left Wheel">
		<attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
		<attnum name="toe" min="-5" max="5" unit="deg" val="0"/>
		<attnum name="camber" min="-2" max="0" unit="deg" val="-2"/>
	</section>
	<section name="Rear Right Wheel">
		<attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
		<attnum name="toe" min="0" max="0" unit="deg" val="0"/>
		<attnum name="camber" min="-2" max="0" unit="deg" val="-2"/>
	</section>
	<section name="Rear Left Wheel">
		<attnum name="ride height" min="100" max="300" unit="mm" val="100"/>
		<attnum name="toe" min="0" max="0" unit="deg" val="0"/>
		<attnum name="camber" min="-2" max="0" unit="deg" val="-2"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" min="4000" max="10000" unit="lbs/in" val="3500"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="180"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="240"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="40"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" min="4000" max="10000" unit="lbs/in" val="3500"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="180"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="240"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="40"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" min="4000" max="10000" unit="lbs/in" val="3000"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="0.8"/>
		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="180"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="240"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="40"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" min="4000" max="10000" unit="lbs/in" val="3000"/>
		<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
		<attnum name="bellcrank" min="0.1" max="5" val="0.8"/>
		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
		<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="180"/>
		<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="240"/>
		<attnum name="fast bump" min="0" max="1000" unit="lbs/in/s" val="40"/>
		<attnum name="fast rebound" min="0" max="1000" unit="lbs/in/s" val="50"/>
	</section>
	<section name="simplix private">
		<attnum name="pit test stop" min="0" max="1" val="0"/>
		<attnum name="qualification" min="0" max="1" val="0"/>
		<attnum name="reserve" min="0.0" max="20000.0" val="2000"/>
		<attnum name="start fuel" min="-1" max="100.0" val="-1"/>
		<attnum name="fuelper100km" min="1.0" max="100.0" val="70.0"/>
		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.05"/>
		<attnum name="avoid width" min="0.0" max="3.0" val="0.5"/>
		<attnum name="scale avoid" min="1.0" max="20.0" val="10.0"/>
		<attnum name="scale brake" min="0" max="1" val="0.68"/>
		<attnum name="scale bump" min="0" max="1" val="0.4"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.6"/>
		<attnum name="scale min mu" min="0.1" max="1.0" val="0.8"/>
		<attnum name="scale mu" min="0" max="2" val="0.78"/>
		<attnum name="scale steer" min="0.1" max="2.0" val="1.0"/>
		<attnum name="stay together" min="10.0" max="2000.0" val="10"/>
		<attnum name="length margin" min="1.0" max="10.0" val="2.0"/>
	</section>
</params>
