<!DOCTYPE params SYSTEM "params.dtd">


<params name="sc-lynx-220">
  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.5" max="0.65" val="0.5346"/>
    <attnum name="max pressure" unit="kPa" min="5000" max="20000" val="20000"/>
  </section>

  <section name="simplix private">
    <attnum name="genetic optimisation" min="0" max="1" val="0"/>
    <attnum name="initial fuel" min="-1" max="90" val="-1"/>
    <attnum name="scale brake" min="0.5" max="1.5" val="0.48"/>
    <attnum name="scale mu" min="0.5" max="1.5" val="0.93"/>
    <attnum name="scale bump" min="0.001" max="0.6" val="0.382"/>
    <attnum name="base mode" min="0.97" max="1.03" val="1.00129"/>
    <attnum name="base scale" min="1" max="1.05" val="1.00853"/>
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" min="0" max="20000" val="2000"/>
    <attnum name="start fuel" min="-1" max="100" val="-1"/>
    <attnum name="fuelper100km" min="1" max="100" val="70"/>
    <attnum name="lookaheadfactor" min="0" max="0.5" val="0.05"/>
    <attnum name="avoid width" min="0" max="3" val="0.25"/>
    <attnum name="scale avoid" min="1" max="20" val="10"/>
    <attnum name="scale bump outer" min="0" max="2" val="0.6"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.7"/>
    <attnum name="scale steer" min="0.1" max="2" val="1"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
    <attnum name="length margin" min="1" max="10" val="2"/>
  </section>

</params>
