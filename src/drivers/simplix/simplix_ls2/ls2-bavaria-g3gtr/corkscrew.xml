<!DOCTYPE params SYSTEM "params.dtd">


<params name="ls2-bavaria-g3gtr">
  <section name="simplix private">
    <attnum name="start fuel" min="-1" max="100" val="100"/>
    <attnum name="scale brake" min="0.5" max="1.5" val="1"/>
    <attnum name="scale mu" min="0.5" max="1.5" val="0.91"/>
    <attnum name="scale bump" min="0.001" max="0.6" val="0.248"/>
    <attnum name="track param count" min="0" max="10" val="2"/>
    <section name="param seg">
      <section name="1">
        <attnum name="apex" min="0.97" max="0.998" val="0.9977"/>
        <attnum name="factor" min="1" max="1.1" val="1.0047"/>
        <attnum name="scale friction" min="0.8" max="1.2" val="1.005"/>
        <attnum name="scale braking" min="0.8" max="1.2" val="1"/>
      </section>

      <section name="2">
        <attnum name="apex" min="0.97" max="0.998" val="0.9977"/>
        <attnum name="factor" min="1" max="1.1" val="1.00349"/>
        <attnum name="scale friction" min="0.8" max="1.2" val="1"/>
        <attnum name="scale braking" min="0.8" max="1.2" val="1"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attnum name="ratio" min="2" max="5" val="3.391"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" min="0.5" max="0.7" val="0.5235"/>
    <attnum name="max pressure" unit="kPa" min="10000" max="23000" val="22800"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="0"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" unit="kN/m" min="0" max="50" val="40"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="61.4"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="2"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="14"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="6"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="15"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="61.4"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="2"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="14"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="6"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="15"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="15"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="4"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="20.8"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="8"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="13"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="kN/m" min="10" max="300" val="15"/>
    <attnum name="slow bump" unit="kN/m/s" min="1" max="100" val="4"/>
    <attnum name="slow rebound" unit="kN/m/s" min="1" max="100" val="20.8"/>
    <attnum name="fast bump" unit="kN/m/s" min="1" max="100" val="8"/>
    <attnum name="fast rebound" unit="kN/m/s" min="1" max="100" val="13"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" min="2" max="20" val="10.93"/>
  </section>

  <section name="Front Wing">
    <attnum name="angle" unit="deg" min="6" max="10" val="10"/>
  </section>

  <section name="Car">
    <attnum name="initial fuel" min="100" max="100" val="100"/>
  </section>

</params>
