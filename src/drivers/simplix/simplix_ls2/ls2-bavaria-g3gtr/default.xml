<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ls2-bavaria-g3gtr" type="template" mode="mw">
      <!-- Car parameters >>> -->
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="6" max="10" val="10.0"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="2" max="20" val="6.0"/>
	</section>

  	<section name="Gearbox">
    		<attnum name="shift time" unit="s" val=".25"/>
    			<section name="gears">
      			<section name="r">
					<attnum name="ratio" min="-3" max="0" val="-2.0"/>
      		</section>

      		<section name="1">
        			<attnum name="ratio" min="0" max="5" val="3.0"/>
      		</section>

      		<section name="2">
        			<attnum name="ratio" min="0" max="5" val="2.22"/>
      		</section>

      		<section name="3">
        			<attnum name="ratio" min="0" max="5" val="1.5"/>
      		</section>

      		<section name="4">
        			<attnum name="ratio" min="0" max="5" val="1.15"/>
      		</section>

      		<section name="5">
        			<attnum name="ratio" min="0" max="5" val="1.00"/>
      		</section>

      		<section name="6">
        			<attnum name="ratio" min="0" max="5" val="0.92"/>
      		</section>
    		</section>
  	</section>

	<section name="Rear Differential">
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="1" max="10" val="3.8"/>
	</section>

	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.57"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="30000"/>
	</section>

	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="20"/>
	</section>

	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="kN/m" min="0" max="50" val="12"/>
	</section>

	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5.0"/>
	</section>

	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-5.0"/>
	</section>

	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-4.0"/>
	</section>

	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="100" max="300" val="100"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.0"/>
		<attnum name="camber" min="-5" max="0" unit="deg" val="-4.0"/>
	</section>

	<section name="Front Right Suspension">
    		<attnum name="spring" min="0" max="300" unit="kN/m" val="71.9"/>
    		<attnum name="suspension course" min="0" max="0.15" unit="m" val="0.15"/>
    		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
    		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    		<attnum name="slow bump" min="0" max="100" unit="kN/m/s" val="9.3"/>
    		<attnum name="slow rebound" min="0" max="100" unit="kN/m/s" val="14.3"/>
    		<attnum name="fast bump" min="0" max="100" unit="kN/m/s" val="2.1"/>
    		<attnum name="fast rebound" min="0" max="100" unit="kN/m/s" val="4.8"/>
	</section>

	<section name="Front Left Suspension">
    		<attnum name="spring" min="0" max="300" unit="kN/m" val="71.9"/>
    		<attnum name="suspension course" min="0" max="0.15" unit="m" val="0.15"/>
    		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
    		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    		<attnum name="slow bump" min="0" max="100" unit="kN/m/s" val="9.3"/>
    		<attnum name="slow rebound" min="0" max="100" unit="kN/m/s" val="14.3"/>
    		<attnum name="fast bump" min="0" max="100" unit="kN/m/s" val="2.1"/>
    		<attnum name="fast rebound" min="0" max="100" unit="kN/m/s" val="4.8"/>
	</section>

	<section name="Rear Right Suspension">
    		<attnum name="spring" min="0" max="300" unit="kN/m" val="53.4"/>
    		<attnum name="suspension course" min="0" max="0.15" unit="m" val="0.15"/>
    		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
    		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    		<attnum name="slow bump" min="0" max="100" unit="kN/m/s" val="7.7"/>
    		<attnum name="slow rebound" min="0" max="100" unit="kN/m/s" val="11.8"/>
    		<attnum name="fast bump" min="0" max="100" unit="kN/m/s" val="2.6"/>
    		<attnum name="fast rebound" min="0" max="100" unit="kN/m/s" val="3.9"/>
	</section>

	<section name="Rear Left Suspension">
    		<attnum name="spring" min="0" max="300" unit="kN/m" val="53.4"/>
    		<attnum name="suspension course" min="0" max="0.15" unit="m" val="0.15"/>
    		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
    		<attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    		<attnum name="slow bump" min="0" max="100" unit="kN/m/s" val="7.7"/>
    		<attnum name="slow rebound" min="0" max="100" unit="kN/m/s" val="11.8"/>
    		<attnum name="fast bump" min="0" max="100" unit="kN/m/s" val="2.6"/>
    		<attnum name="fast rebound" min="0" max="100" unit="kN/m/s" val="3.9"/>
	</section>

      <!-- <<< Car parameters -->

      <!-- Robot parameters >>> -->
	<section name="simplix private">
            <!-- Use this parameters to test the car's pitting -->
		<attnum name="pit test stop" min="0" max="1" val="0"/>
		<attnum name="qualification" min="0" max="1" val="0"/>

            <!-- Use this parameters to setup the car's fuel strategy -->
		<attnum name="reserve" unit="m" min="0.0" max="20000.0" val="2000"/>
		<attnum name="start fuel" min="-1" max="60.0" val="-1"/>
		<attnum name="fuelper100km" min="1.0" max="100.0" val="80.0"/>

            <!-- Use the parameters below to tune the robots 
                 detection of the car's limits -->
		<attnum name="accel out" min="0" max="1" val="1"/>
		<attnum name="clutch max" min="0.001" max="1.0" val="0.5"/>
		<attnum name="fly height" min="0.01" max="1.0" val="0.20"/>
		<attnum name="jump offset" min="0" max="0.1" val="0.04"/>
		<attnum name="tcl slip" min="1.0" max="10.0" val="1.0"/>
		<attnum name="tcl factor" min="1.0" max="10.0" val="1.0"/>
		<attnum name="drift factor" min="1.0" max="10.0" val="1.0"/>
		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.10"/>
		<attnum name="scale steer" min="0.1" max="2.0" val="0.8"/>
		<attnum name="brake limit" val="-9.9"/>
		<attnum name="scale brake" min="0" max="2" val="0.76"/>
		<attnum name="scale mu" min="0" max="5" val="0.81"/>
		<attnum name="scale brake rain" min="0" max="1" val="1.0"/>
		<attnum name="scale mu rain" min="0" max="1" val="1.0"/>
    		<attnum name="scale bump" val="0.3"/>
    		<attnum name="scale bump outer" val="0.45"/>

            <!-- Use the parameters below to tune the robots 
                 detection of the car's limits while qualification -->
		<attnum name="qualy brake" min="0" max="2" val="0.78"/>
		<attnum name="qualy mu" min="0" max="2" val="0.85"/>

            <!-- Do not change the parameters below 
                 unless you really know what you are doing -->
		<attnum name="length margin" min="1.0" max="10.0" val="3.0"/>
		<attnum name="avoid width" min="0.0" max="3.0" val="0.5"/>
		<attnum name="scale avoid" min="1.0" max="20.0" val="20.0"/>
		<attnum name="scale min mu" min="0.1" max="1.0" val="0.8"/>
		<attnum name="stay together" min="10.0" max="2000.0" val="10"/>
	</section>
      <!-- <<< Robot parameters -->
</params>
