<!DOCTYPE params SYSTEM "params.dtd">


<params name="ls1-archer-r9">
  <section name="Front Wing">
    <attnum name="angle" unit="deg" val="19.3"/>
  </section>

  <section name="Rear Wing">
    <attnum name="angle" unit="deg" val="13"/>
  </section>

  <section name="Gearbox">
    <section name="gears">
      <section name="r">
        <attnum name="ratio" min="-3" max="0" val="-3"/>
      </section>

      <section name="1">
        <attnum name="ratio" min="0" max="9" val="3.7"/>
      </section>

      <section name="2">
        <attnum name="ratio" min="0" max="5" val="2.85"/>
      </section>

      <section name="3">
        <attnum name="ratio" min="0" max="5" val="2.195"/>
      </section>

      <section name="4">
        <attnum name="ratio" min="0" max="5" val="1.69"/>
      </section>

      <section name="5">
        <attnum name="ratio" min="0" max="5" val="1.3"/>
      </section>

      <section name="6">
        <attnum name="ratio" min="0" max="5" val="1"/>
      </section>

    </section>

  </section>

  <section name="Rear Differential">
    <attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
    <attnum name="ratio" val="3.38"/>
  </section>

  <section name="Brake System">
    <attnum name="front-rear brake repartition" val="0.69"/>
    <attnum name="max pressure" unit="kPa" val="47100"/>
  </section>

  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="200" max="2500" unit="lbs/in" val="2325"/>
  </section>

  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="1" max="2500" unit="lbs/in" val="1"/>
  </section>

  <section name="Front Right Wheel">
    <attnum name="ride height" min="100" max="140" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="-0.3"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Front Left Wheel">
    <attnum name="ride height" min="100" max="140" unit="mm" val="100"/>
    <attnum name="toe" min="-5" max="5" unit="deg" val="0.3"/>
    <attnum name="camber" min="-5" max="0" unit="deg" val="-5"/>
  </section>

  <section name="Rear Right Wheel">
    <attnum name="ride height" min="100" max="140" unit="mm" val="100"/>
    <attnum name="toe" min="-1" max="1" unit="deg" val="0"/>
    <attnum name="camber" min="-4" max="0" unit="deg" val="-4"/>
  </section>

  <section name="Rear Left Wheel">
    <attnum name="ride height" min="100" max="140" unit="mm" val="100"/>
    <attnum name="toe" min="-1" max="1" unit="deg" val="0"/>
    <attnum name="camber" min="-4" max="0" unit="deg" val="-4"/>
  </section>

  <section name="Front Right Suspension">
    <attnum name="spring" min="100" max="10000" unit="lbs/in" val="5900"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="0.94"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="400" unit="lbs/in/s" val="80"/>
    <attnum name="slow rebound" min="0" max="400" unit="lbs/in/s" val="380"/>
    <attnum name="fast bump" min="0" max="400" unit="lbs/in/s" val="155"/>
    <attnum name="fast rebound" min="0" max="400" unit="lbs/in/s" val="300"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" min="100" max="10000" unit="lbs/in" val="5900"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="0.94"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="400" unit="lbs/in/s" val="80"/>
    <attnum name="slow rebound" min="0" max="400" unit="lbs/in/s" val="380"/>
    <attnum name="fast bump" min="0" max="400" unit="lbs/in/s" val="155"/>
    <attnum name="fast rebound" min="0" max="400" unit="lbs/in/s" val="300"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" min="100" max="10000" unit="lbs/in" val="4800"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="1.19"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="400" unit="lbs/in/s" val="155"/>
    <attnum name="slow rebound" min="0" max="400" unit="lbs/in/s" val="230"/>
    <attnum name="fast bump" min="0" max="400" unit="lbs/in/s" val="65"/>
    <attnum name="fast rebound" min="0" max="400" unit="lbs/in/s" val="170"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" min="100" max="10000" unit="lbs/in" val="4800"/>
    <attnum name="suspension course" min="0" max="0.28" unit="m" val="0.2"/>
    <attnum name="bellcrank" val="1.19"/>
    <attnum name="packers" min="0" max="10" unit="mm" val="0"/>
    <attnum name="slow bump" min="0" max="400" unit="lbs/in/s" val="155"/>
    <attnum name="slow rebound" min="0" max="400" unit="lbs/in/s" val="230"/>
    <attnum name="fast bump" min="0" max="400" unit="lbs/in/s" val="65"/>
    <attnum name="fast rebound" min="0" max="400" unit="lbs/in/s" val="170"/>
  </section>

  <section name="simplix private">
    <attnum name="pit test stop" min="0" max="1" val="0"/>
    <attnum name="qualification" min="0" max="1" val="0"/>
    <attnum name="reserve" min="0" max="20000" unit="m" val="2000"/>
    <attnum name="start fuel" min="-1" max="60" val="-1"/>
    <attnum name="fuelper100km" min="1" max="100" val="80"/>
    <attnum name="_scale bump" min="0" max="2" val="0.6"/>
    <attnum name="_scale bump outer" min="0" max="2" val="0.8"/>
    <attnum name="accel out" min="0" max="1" val="1"/>
    <attnum name="clutch max" min="0.001" max="1" val="0.45"/>
    <attnum name="fly height" min="0.01" max="1" val="0.2"/>
    <attnum name="jump offset" min="0" max="0.1" val="0.04"/>
    <attnum name="tcl slip" min="1" max="10" val="3.3"/>
    <attnum name="tcl factor" min="1" max="10" val="1"/>
    <attnum name="drift factor" min="1" max="10" val="1.1"/>
    <attnum name="lookaheadfactor" min="0" max="0.5" val="0.05"/>
    <attnum name="scale steer" min="0.1" max="2" val="1"/>
    <attnum name="brake limit" val="-19"/>
    <attnum name="scale brake" val="1.25"/>
    <attnum name="scale mu" val="1.24"/>
    <attnum name="scale brake rain" min="0" max="1" val="1"/>
    <attnum name="scale mu rain" min="0" max="5" val="1"/>
    <attnum name="qualy brake" val="1.3"/>
    <attnum name="qualy mu" val="1.29"/>
    <attnum name="length margin" min="1" max="10" val="3"/>
    <attnum name="avoid width" min="0" max="3" val="0.5"/>
    <attnum name="scale avoid" min="1" max="20" val="15"/>
    <attnum name="scale min mu" min="0.1" max="1" val="0.8"/>
    <attnum name="stay together" min="10" max="2000" val="10"/>
  </section>

</params>
