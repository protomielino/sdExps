<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="ls1-cavallo-570s1" type="template" mode="mw">
      <!-- Car parameters >>> -->
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="21" val="18.3"/>
	</section>
	
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="24" val="24.0"/>
	</section>

	<section name="Rear Differential">
		<attnum name="ratio" min="1.0" max="10.00" val="3.65"/>
	</section>
	
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.55"/>
		<attnum name="max pressure" unit="kPa" min="100" max="150000" val="44700"/>
	</section>

	<section name="Front Right Suspension">
		<attnum name="bellcrank" min="0.1" max="2.0" val="0.56"/>
	</section>

	<section name="Front Left Suspension">
		<attnum name="bellcrank" min="0.1" max="2.0" val="0.56"/>
	</section>

	<section name="Rear Right Suspension">
		<attnum name="bellcrank" min="0.1" max="2.0" val="1.215"/>
	</section>

	<section name="Rear Left Suspension">
		<attnum name="bellcrank" min="0.1" max="2.0" val="1.215"/>
	</section>
      <!-- <<< Car parameters -->

      <!-- Robot parameters >>> -->
	<section name="simplix private">
            <!-- Use this parameters to test the car's pitting -->
		<attnum name="pit test stop" min="0" max="1" val="0"/>
		<attnum name="qualification" min="0" max="1" val="0"/>

            <!-- Use this parameters to setup the car's fuel strategy -->
		<attnum name="reserve" unit="m" min="0.0" max="20000.0" val="2000"/>
		<attnum name="start fuel" min="-1" max="60.0" val="-1"/>
		<attnum name="fuelper100km" min="1.0" max="100.0" val="80.0"/>

            <!-- Use the parameters below to tune the robots detection
                 of changes in the track's height -->
		<attnum name="scale bump" min="0.0" max="2.0" val="0.3"/>
		<attnum name="scale bump outer" min="0.0" max="2.0" val="0.5"/>

            <!-- Use the parameters below to tune the robots 
                 detection of the car's limits -->
		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.20"/>
		<attnum name="scale brake" min="0" max="1" val="1.01"/>
		<attnum name="scale mu" min="0" max="5" val="1.10"/>

            <!-- Use the parameters below to tune the robots 
                 detection of the car's limits while qualification -->
		<attnum name="qualy brake" min="0" max="1" val="1.06"/>
		<attnum name="qualy mu" min="0" max="2" val="1.15"/>

	</section>
      <!-- <<< Robot parameters -->
</params>
