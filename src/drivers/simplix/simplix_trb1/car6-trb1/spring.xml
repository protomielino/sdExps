<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="car6-trb1" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="9.0"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="17" val="12.3"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="750"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3500"/>
	</section>
	<section name="simplix private">
		<attnum name="scale brake" min="0" max="1" val="0.80"/>
		<attnum name="scale bump" min="0" max="1" val="0.2"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.4"/>
		<attnum name="scale mu" min="0" max="2" val="0.80"/>
	</section>
</params>
