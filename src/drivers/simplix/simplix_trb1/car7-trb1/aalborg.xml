<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="car7-trb1" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="12"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="18" val="18"/>
	</section>
	<section name="Rear Differential">
		<attnum name="ratio" min="0" max="10" val="4.25"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="500"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
  <section name="Front Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"/>
  </section>

  <section name="Front Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="2500"/>
  </section>

  <section name="Rear Right Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"/>
  </section>

  <section name="Rear Left Suspension">
    <attnum name="spring" unit="lbs/in" min="0" max="10000" val="3000"/>
  </section>
	<section name="simplix private">
		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.03"/>
		<attnum name="scale brake" min="0" max="1" val="0.65"/>
		<attnum name="scale bump" min="0" max="1" val="0.01"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.10"/>
		<attnum name="scale min mu" min="0.1" max="1.0" val="0.8"/>
		<attnum name="scale mu" min="0" max="2" val="0.63"/>
	</section>
</params>
