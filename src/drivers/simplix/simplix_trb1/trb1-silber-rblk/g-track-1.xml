<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-silber-rblk" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="11" val="11"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="16" val="16"/>
	</section>
	<section name="Rear Differential">
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="FREE"/>
		<attnum name="ratio" min="0" max="10" val="5.0"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="2000"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="5500"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="5500"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="3500"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" min="0" max="10000" unit="lbs/in" val="3500"/>
	</section>
	<section name="simplix private">
		<attnum name="lookaheadfactor" min="0.0" max="0.5" val="0.20"/>
		<attnum name="scale brake" min="0" max="1" val="0.6"/>
		<attnum name="scale bump" min="0" max="1" val="0.30"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.50"/>
		<attnum name="scale mu" min="0" max="2" val="0.85"/>
	</section>
</params>

