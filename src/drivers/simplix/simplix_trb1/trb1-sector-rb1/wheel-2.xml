<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-sector-rb1" type="template" mode="mw">
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.56"/>
	</section>
	<section name="Front Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="300"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="0"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.10"/>
		<attnum name="bellcrank" min="0.1" max="5" val="0.85"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.10"/>
		<attnum name="bellcrank" min="0.1" max="5" val="0.85"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.15"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="suspension course" unit="m" min="0" max="0.2" val="0.15"/>
		<attnum name="bellcrank" min="0.1" max="5" val="1.0"/>
	</section>
	<section name="simplix private">
		<attnum name="scale brake" min="0" max="1" val="0.80"/>
		<attnum name="scale mu" min="0" max="2" val="0.90"/>
	</section>
</params>
