<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-sector-rb1" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="10.5"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="17" val="14.5"/>
	</section>
	<section name="Rear Differential">
		<attnum name="ratio" min="0" max="10" val="3.89"/>
	</section>
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.46"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3800"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3800"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3800"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="3800"/>
	</section>
	<section name="simplix private">
		<attnum name="scale brake" min="0" max="1" val="0.70"/>
		<attnum name="scale mu" min="0" max="2" val="0.90"/>
		<attnum name="scale bump" min="0" max="1" val="0.10"/>
		<attnum name="scale bump outer" min="0" max="2" val="0.20"/>
	</section>
</params>
