<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-boxer-trbrs" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="12"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="18" val="18"/>
	</section>
	<section name="Rear Differential">
		<attstr name="type" in="SPOOL,FREE,LIMITED SLIP" val="LIMITED SLIP"/>
		<attnum name="ratio" min="0" max="10" val="3.5"/>
	</section>
	<section name="Front Right Suspension">
		<attnum name="bellcrank" min="0.1" max="5" val="0.75"/>
	</section>
	<section name="Front Left Suspension">
		<attnum name="bellcrank" min="0.1" max="5" val="0.75"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="bellcrank" min="0.1" max="5" val="0.9"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="bellcrank" min="0.1" max="5" val="0.9"/>
	</section>
	<section name="simplix private">
		<attnum name="fuelper100km" val="75.0"/>
		<attnum name="scale brake" min="0" max="1" val="0.7"/>
		<attnum name="scale mu" min="0" max="2" val="0.75"/>
	</section>
</params>
