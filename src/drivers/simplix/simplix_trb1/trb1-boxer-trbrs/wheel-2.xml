<!DOCTYPE params SYSTEM "../../../src/libs/tgf/params.dtd">
<params name="trb1-boxer-trbrs" type="template" mode="mw">
	<section name="Front Wing">
		<attnum name="angle" unit="deg" min="0" max="12" val="9"/>
	</section>
	<section name="Rear Wing">
		<attnum name="angle" unit="deg" min="0" max="18" val="13"/>
	</section>
	<section name="Rear Differential">
		<attnum name="ratio" min="0" max="10" val="3.15"/>
	</section>
	<section name="Rear Anti-Roll Bar">
		<attnum name="spring" unit="lbs/in" min="0" max="5000" val="1500"/>
	</section>
	<section name="Rear Right Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="4500"/>
	</section>
	<section name="Rear Left Suspension">
		<attnum name="spring" unit="lbs/in" min="0" max="10000" val="4500"/>
	</section>
</params>
